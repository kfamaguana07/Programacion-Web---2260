@{
	ViewData["Title"] = "Laboratorio";
}

<h1>Laboratorio</h1>

<fieldset class="border p-3">
    <legend class="h5">Buscar Laboratorio</legend>
    <form id="frmBusqueda" method="post" class="row g-3">
        <div class="col-md-3">
            <label for="inputNombre" class="form-label">Nombre</label>
            <input type="text" class="form-control" name="nombre" id="inputNombre">
        </div>
        <div class="col-md-3">
            <label for="inputDireccion" class="form-label">Dirección</label>
            <input type="text" class="form-control" name="direccion" id="inputDireccion">
        </div>
        <div class="col-md-3">
            <label for="inputDireccion" class="form-label">Persona Contacto</label>
            <input type="text" class="form-control" name="personaContacto" id="inputContacto">
        </div>
        <div class="col-12">
            <button type="button" class="btn btn-outline-danger" onclick="BuscarLaboratorio()">Buscar</button>
            <button type="button" class="btn btn-outline-success" onclick="LimpiarLaboratorio()">Limpiar</button>
        </div>
    </form>
</fieldset>


<div id="divTabla"></div>

<script src="~/js/generic.js"></script>
<script src="~/js/laboratorio.js"></script>